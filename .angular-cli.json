{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "citas-admin"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../src/assets/vendor/bootstrap/css/bootstrap.min.css",
        "../src/assets/css/style2.css",
        "../src/assets/vendor/font-awesome/css/font-awesome.min.css",
        "../src/assets/vendor/datatables/dataTables.bootstrap4.css",
        "../src/assets/css/sb-admin.min.css",
        "styles.css"

      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.slim.min.js",
        "../src/assets/vendor/tether/tether.min.js",
        "../src/assets/vendor/bootstrap/js/bootstrap.min.js",
        "../src/assets/vendor/jquery-easing/jquery.easing.min.js",
        "../src/assets/vendor/datatables/jquery.dataTables.js",
        "../src/assets/vendor/datatables/dataTables.bootstrap4.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
